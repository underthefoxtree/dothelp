{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "go: build release",
			"command": "go",
			"args": [
				"build",
				"-o",
				"${workspaceFolder}",
				"-ldflags",
				"-s -w",
			],
			"problemMatcher": [
				"$go"
			],
			"options": {
				"cwd": "${workspaceFolder}/src/"
			},
			"group": "build",
		}
	]
}